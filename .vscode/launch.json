{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "envFile": "${workspaceFolder}/.env",
      "program": "./src/index.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm start",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start:dev",
      "request": "launch",
      "command": "npm run start:dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}