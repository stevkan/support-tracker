{
  "name": "support-tracker",
  "version": "2.7.2",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "support-tracker": "src/index.js"
  },
  "scripts": {
    "cli": "node src/index.js",
    "migrate:secrets": "dotenvx run --env-file .env -- node scripts/migrateSecrets.js",
    "dev": "npm run update-version && concurrently -k \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:electron": "cross-env NODE_ENV=development electron src/main/main.js",
    "dev:renderer": "vite --config src/renderer/vite.config.js",
    "build:renderer": "vite build --config src/renderer/vite.config.js",
    "build:electron": "npm run build:renderer && electron-builder",
    "package": "npm run update-version &&npm run build:electron",
    "update-version": "node scripts/update-version.js"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "applicationinsights": "^2.1.8",
    "axios": "^1.7.3",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^8.0.0",
    "env-paths": "^4.0.0",
    "fastify": "^5.0.0",
    "html-to-text": "^9.0.5",
    "jspdf": "^4.0.0",
    "keytar": "^7.9.0",
    "lowdb": "^7.0.1",
    "open-web-browser": "^0.1.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "storaje-db": "github:stevkan/storajeDb"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "vite": "^5.4.0"
  }
}
